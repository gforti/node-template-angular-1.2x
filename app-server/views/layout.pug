doctype html
html
  head
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title= title 
    each val in bundle 
        if val.styles
            link( href=val.styles, media='all', rel='stylesheet', type='text/css' )    
  body(ng-app=moduleName)
    .container
        div(ui-view ng-cloak)
            block content
    
    each val in bundle
        if val.scripts
            script( src=val.scripts, type='text/javascript' )
      
